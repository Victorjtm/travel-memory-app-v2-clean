{
    "name": "travel-memory-app",
    "version": "0.0.0",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "start": "npm-run-all --parallel start:backend start:frontend",
        "start:frontend": "ng serve --host 0.0.0.0 --disable-host-check --configuration mobile",
        "start:https": "ng serve --ssl --host 0.0.0.0 --disable-host-check",
        "start:full-https": "npm-run-all --parallel start:backend start:https",
        "start:backend": "node src/app/server.js",
        "start:prod": "npm run build && cross-env NODE_ENV=production PORT=4000 serve -s dist/travel-memory-app --single",
        "ngrok": "start-ngrok.bat",
        "electron": "electron ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.1.0",
        "@angular/cdk": "^19.2.18",
        "@angular/common": "^19.1.0",
        "@angular/compiler": "^19.1.0",
        "@angular/core": "^19.1.0",
        "@angular/forms": "^19.1.0",
        "@angular/material": "^19.2.18",
        "@angular/platform-browser": "^19.1.0",
        "@angular/platform-browser-dynamic": "^19.1.0",
        "@angular/router": "^19.1.0",
        "@fortawesome/angular-fontawesome": "^1.0.0",
        "@fortawesome/fontawesome-free": "^7.0.1",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@types/leaflet": "^1.9.20",
        "browser-image-compression": "^2.0.2",
        "cors": "^2.8.5",
        "exif-js": "^2.3.0",
        "exif-parser": "^0.1.12",
        "exiftool-vendored": "^30.2.0",
        "express": "^4.18.2",
        "jszip": "^3.10.1",
        "leaflet": "^1.9.4",
        "multer": "^2.0.0",
        "ngx-lightbox": "^3.0.0",
        "rxjs": "~7.8.0",
        "sqlite3": "^5.1.7",
        "tslib": "^2.3.0",
        "viewerjs": "^1.11.7",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.1.8",
        "@angular/cli": "^19.1.8",
        "@angular/compiler-cli": "^19.1.0",
        "@types/exif-js": "^2.3.1",
        "@types/jasmine": "~5.1.0",
        "@types/jszip": "^3.4.0",
        "cross-env": "^7.0.3",
        "electron": "^39.2.7",
        "electron-builder": "^26.0.12",
        "jasmine-core": "~5.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "npm-run-all": "^4.1.5",
        "typescript": "~5.7.2"
    }
}
